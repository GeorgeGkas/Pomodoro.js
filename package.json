{
  "name": "pomodorojs",
  "version": "2.0.0",
  "description": "A javascript time management library for busy humans.",
  "main": "Pomodoro.npm.js",
  "scripts": {
    "build": "babel --no-comments  --compact --minified Pomodoro.js -o Pomodoro.npm.js",
    "unit": "mocha --compilers js:babel-register --recursive ./test/unit/",
    "eslint": "eslint --ext js Pomodoro.js test/",
    "cover": "node_modules/.bin/babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha -- --recursive ./test/unit/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GeorgeGkas/Pomodoro.js.git"
  },
  "author": "George Gkasdrogkas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GeorgeGkas/Pomodoro.js/issues"
  },
  "homepage": "https://github.com/GeorgeGkas/Pomodoro.js#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-istanbul": "^0.12.2",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.0.2",
    "eslint": "^4.2.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^3.4.2"
  }
}
